{
  "extends": [
    "@tsconfig/node18/tsconfig.json",
    "@vue/tsconfig/tsconfig.json"
  ],
  "include": [
    "vite.config.*",
    "vitest.config.*",
    "cypress.config.*",
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "node",
      "vitest/globals"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  }
}